{
  "multipart": [
    {
      "when": {
        "facing": "down"
      },
      "apply": {
        "model": "on1chest:block/item_exporter",
        "x": 180
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "on1chest:block/item_exporter",
        "x": 90,
        "y": 90
      }
    },
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "on1chest:block/item_exporter",
        "x": 90
      }
    },
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "on1chest:block/item_exporter",
        "x": 90,
        "y": 180
      }
    },
    {
      "when": {
        "facing": "up"
      },
      "apply": {
        "model": "on1chest:block/item_exporter"
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "on1chest:block/item_exporter",
        "x": 90,
        "y": 270
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "on1chest:block/connection_cable_open",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "on1chest:block/connection_cable_open",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "on1chest:block/connection_cable_open",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "on1chest:block/connection_cable_open",
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "on1chest:block/connection_cable_open",
        "x": -90,
        "uvlock": true
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "on1chest:block/connection_cable_open",
        "x": 90,
        "uvlock": true
      }
    }
  ]
}
