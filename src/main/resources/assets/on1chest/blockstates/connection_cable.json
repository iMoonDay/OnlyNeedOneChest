{
  "multipart": [
    {
      "apply": {
        "model": "on1chest:block/connection_cable_base"
      }
    },
    {
      "when": {
        "north": "false"
      },
      "apply": {
        "model": "on1chest:block/connection_cable_closed",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "false"
      },
      "apply": {
        "model": "on1chest:block/connection_cable_closed",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "false"
      },
      "apply": {
        "model": "on1chest:block/connection_cable_closed",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "false"
      },
      "apply": {
        "model": "on1chest:block/connection_cable_closed",
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "up": "false"
      },
      "apply": {
        "model": "on1chest:block/connection_cable_closed",
        "x": -90,
        "uvlock": true
      }
    },
    {
      "when": {
        "down": "false"
      },
      "apply": {
        "model": "on1chest:block/connection_cable_closed",
        "x": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "on1chest:block/connection_cable_open",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "on1chest:block/connection_cable_open",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "on1chest:block/connection_cable_open",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "on1chest:block/connection_cable_open",
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "on1chest:block/connection_cable_open",
        "x": -90,
        "uvlock": true
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "on1chest:block/connection_cable_open",
        "x": 90,
        "uvlock": true
      }
    }
  ]
}
